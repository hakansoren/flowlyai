# Twilio Configuration
TWILIO_ACCOUNT_SID=ACxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxx
TWILIO_AUTH_TOKEN=xxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxx
TWILIO_PHONE_NUMBER=+15551234567

# Webhook Configuration
WEBHOOK_BASE_URL=https://your-ngrok-url.ngrok.io
WEBHOOK_PORT=8765

# STT Provider (groq, deepgram, openai, or elevenlabs)
STT_PROVIDER=groq

# Deepgram Configuration (if STT_PROVIDER=deepgram or TTS_PROVIDER=deepgram)
DEEPGRAM_API_KEY=xxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxx

# Groq Configuration (if STT_PROVIDER=groq) - Fast Whisper
GROQ_API_KEY=gsk_xxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxx

# OpenAI Configuration (for TTS and optional STT)
OPENAI_API_KEY=sk-xxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxx

# ElevenLabs Configuration (for STT and/or TTS)
# Get key at: https://elevenlabs.io/app/settings/api-keys
ELEVENLABS_API_KEY=xxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxx

# Flowly Agent Connection
FLOWLY_GATEWAY_URL=http://localhost:18790

# TTS Provider (elevenlabs, openai, or deepgram)
TTS_PROVIDER=elevenlabs

# Voice Settings
# For ElevenLabs: use voice IDs (e.g., 21m00Tcm4TlvDq8ikWAM for rachel)
#   rachel, bella, elli (female) | josh, adam, sam (male)
# For OpenAI: nova, alloy, echo, fable, onyx, shimmer
# For Deepgram: aura-asteria-en, aura-luna-en, aura-orion-en, etc.
TTS_VOICE=21m00Tcm4TlvDq8ikWAM
TTS_MODEL=eleven_multilingual_v2
LANGUAGE=en-US

# Logging
LOG_LEVEL=info
